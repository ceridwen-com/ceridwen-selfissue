          <xs:attribute name="class" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="com.ceridwen.selfissue.client.nulldevices.IDReaderDevice"/>
                <xs:enumeration value="com.ceridwen.selfissue.client.devices.JavaSmartCardDevice"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="ItemIDReaderDevice">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="SmartCardReader">
          <xs:attribute name="class" use="required">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="com.ceridwen.selfissue.client.nulldevices.IDReaderDevice"/>
                <xs:enumeration value="com.ceridwen.selfissue.client.devices.JavaSmartCardDevice"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
