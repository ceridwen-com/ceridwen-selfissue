        <xs:element ref="StrictChecksumChecking"/>
        <xs:element ref="StrictSequenceChecking"/>
        <xs:element ref="InstitutionId"/>
        <xs:element ref="TerminalPassword"/>
        <xs:element ref="SendEndPatronSession"/>
        <xs:element ref="RequirePatronPassword"/>
      </xs:sequence>
      <xs:attribute name="mode" use="required" form="unqualified">
        <xs:annotation>
          <xs:documentation>Socket or Telnet</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="Socket"/>
