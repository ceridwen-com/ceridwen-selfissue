  <xs:element name="SIP">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Host"/>
        <xs:element ref="Port"/>
        <xs:element ref="TelnetUsername"/>
        <xs:element ref="TelnetPassword"/>
        <xs:element ref="LoggedOnText"/>
        <xs:element ref="LoginUserId"/>
